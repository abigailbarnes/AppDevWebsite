<h1>List of Users</h1>

<table border = "1">
  <tr>
    <th>
      Name
    </th>

    <th>
      Email
    </th>

  </tr>

  <% @list_of_users.each do |a_user|%>
    <tr>

      <td>
        <%= a_user.first_name %>
        <%= a_user.last_name %>
      </td>

      <td>
        <%= a_user.email %>
      </td>
    </tr>
  <% end %>
</table>