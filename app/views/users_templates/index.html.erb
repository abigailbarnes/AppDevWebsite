<h1>List of Users</h1>

<table border = "1">
  <tr>
    <th>
      ID
    </th>

    <th>
      Email
    </th>

    <th>
      
    </th>
  </tr>

  <% @list_of_users.each do |a_user|%>
    <tr>
      <td>
        <%= a_user.id %>
      </td>

      <td>
        <%= a_user.email %>
      </td>

      <td>
        <a href="/users/<%= a_user.id %>">
          Show details
        </a>
      </td>
    </tr>
  <% end %>
</table>